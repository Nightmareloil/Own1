local CoreGui = game:GetService("CoreGui")

-- Go through all ScreenGui instances in CoreGui
for _, screenGui in ipairs(CoreGui:GetChildren()) do
	if screenGui:IsA("ScreenGui") then
		-- Make 'hub' visible if it exists
		local hub = screenGui:FindFirstChild("hub")
		if hub and hub:IsA("GuiObject") then
			hub.Visible = true
			print("‚úÖ Made Exclusive Hub visible in: " .. screenGui:GetFullName())
		else
			warn("‚ö†Ô∏è No hub found in: " .. screenGui:GetFullName())
		end

		-- After 3 seconds, destroy 'Key' if it exists
		task.delay(3, function()
			local key = screenGui:FindFirstChild("Key")
			if key then
				key:Destroy()
				print("üóëÔ∏è Destroyed Key in: " .. screenGui:GetFullName())
			else
				warn("‚ö†Ô∏è No Key found in: " .. screenGui:GetFullName())
			end
		end)
	end
end
